{
  "name": "familie-klage-frontend",
  "version": "0.0.1",
  "main": "dist/server.js",
  "repository": "https://github.com/navikt/familie-klage-frontend.git",
  "author": "Nav - teamfamilie",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "yarn set-node-options NODE_ENV=production node dist/backend/server.js",
    "start:dev": "tsc -p tsconfig.backend.json && yarn set-node-options NODE_ENV=development node dist/backend/server-lokal.js",
    "test": "vitest",
    "build": "yarn lint && yarn build:prod",
    "build:dev": "tsc -p tsconfig.backend.json & NODE_ENV=development webpack --config ./.nais/webpack/webpack.dev.js",
    "build:prod": "tsc -p tsconfig.backend.json & NODE_ENV=production webpack --config ./.nais/webpack/webpack.prod.js",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "prettier": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "typecheck": "tsc --noEmit -p tsconfig.frontend.json --skipLibCheck && tsc --noEmit -p tsconfig.backend.json --skipLibCheck",
    "validate": "yarn typecheck && yarn lint && yarn prettier",
    "prepare": "husky install",
    "set-node-options": "NODE_OPTIONS='--import=./dist/backend/register.js --es-module-specifier-resolution=node'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "2.2.3",
    "@navikt/ds-css": "7.39.0",
    "@navikt/ds-react": "8.5.0",
    "@navikt/familie-backend": "10.1.20",
    "@navikt/familie-logging": "7.0.7",
    "@navikt/land-verktoy": "9.1.31",
    "@sentry/browser": "10.38.0",
    "@sentry/core": "10.38.0",
    "autoprefixer": "10.4.24",
    "axios": "1.13.5",
    "babel-loader": "10.0.0",
    "body-parser": "2.2.2",
    "constate": "3.3.3",
    "date-fns": "4.1.0",
    "dotenv": "17.3.1",
    "express": "5.2.1",
    "express-static-gzip": "3.0.0",
    "file-loader": "6.2.0",
    "http-proxy-middleware": "3.0.5",
    "prom-client": "15.1.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "react-pdf": "10.3.0",
    "react-router-dom": "7.13.0",
    "svg-inline-loader": "0.8.2",
    "ts-node": "10.9.2",
    "uuid": "13.0.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/preset-env": "7.29.0",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@pmmmwh/react-refresh-webpack-plugin": "0.6.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/express": "5.0.6",
    "@types/node": "24.10.4",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "11.0.0",
    "@types/webpack": "5.28.5",
    "@types/webpack-env": "1.18.8",
    "@types/webpack-hot-middleware": "2.25.12",
    "@vitejs/plugin-react": "5.1.4",
    "compression-webpack-plugin": "11.1.0",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "7.1.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-webpack-plugin": "5.0.2",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "html-webpack-plugin": "5.6.6",
    "husky": "9.1.7",
    "jsdom": "28.0.0",
    "lint-staged": "16.2.7",
    "mini-css-extract-plugin": "2.10.0",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "prettier": "3.8.1",
    "react-refresh": "0.18.0",
    "style-loader": "4.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vitest": "4.0.18",
    "webpack": "5.105.2",
    "webpack-cli": "6.0.1",
    "webpack-dev-middleware": "7.4.5",
    "webpack-dev-server": "5.2.3",
    "webpack-hot-middleware": "2.26.1",
    "webpack-merge": "6.0.1"
  }
}
